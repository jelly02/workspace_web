/* ul, ol 아이템 목록 */
ul.menu_item_1, ol.menu_item_1 {
	list-style: none;
	margin: 0;
	padding: 0;
}

/* ul li, ol li 설정 : #efefef */
ul.menu_item_1 li, ol.menu_item_1 li {
	padding: 5px 0 5px 5px;;
	margin-bottom: 5px;
	border-bottom: dotted 1px #efefef;
	font-size: 12px;
}

/* ul li, ol li 맨마지막 li 항목은 밑줄 제거 */
ul.menu_item_1 li:last-child, ol.menu_item_1 li:last-child {
	border-bottom: 0px;
}